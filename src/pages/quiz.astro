---
import Quiz from '../components/Quiz';
import '../index.css';

// No layout - clean quiz experience
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/vite.svg" />
		<title>AI Automation Quiz | Collin Wilkins</title>
		<meta name="description" content="Discover which AI automation solution fits your business. Get a personalized action plan." />
		<link rel="canonical" href="https://collinwilkins.com/quiz" />

		<!-- Open Graph / Facebook -->
		<meta property="og:type" content="website" />
		<meta property="og:url" content="https://collinwilkins.com/quiz" />
		<meta property="og:title" content="AI Automation Quiz | Collin Wilkins" />
		<meta property="og:description" content="Discover which AI automation solution fits your business. Get a personalized action plan." />
		<meta property="og:image" content="https://collinwilkins.com/images/og-default.png" />

		<!-- Twitter -->
		<meta property="twitter:card" content="summary_large_image" />
		<meta property="twitter:title" content="AI Automation Quiz | Collin Wilkins" />
		<meta property="twitter:description" content="Discover which AI automation solution fits your business. Get a personalized action plan." />
		<meta property="twitter:image" content="https://collinwilkins.com/images/og-default.png" />

		<!-- Google Fonts -->
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
